{
  "name": "Getting Started Example",
  "version": "0.1",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": ["content.bundle.js"],
      "css": ["content.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_title": "Chrome Extension Popup",
    "default_icon": {
      "16": "assets/16.png",
      "32": "assets/32.png",
      "48": "assets/48.png",
      "128": "assets/128.png"
    }
  },
  "icons": {
    "16": "assets/16.png",
    "32": "assets/32.png",
    "48": "assets/48.png",
    "128": "assets/128.png"
  }
}
